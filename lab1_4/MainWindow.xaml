<Window x:Class="lab1_4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab1_4"
        mc:Ignorable="d"
        Title="MainWindow" Height="134" Width="471">
    <Grid Margin="0,0,10,-6">
        <ComboBox x:Name="yearBox" HorizontalAlignment="Left" Margin="14,45,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="Select_Box"/>
        <ComboBox x:Name="monthBox" HorizontalAlignment="Left" Margin="167,45,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="Select_Box"/>
        <ComboBox x:Name="daysBox" HorizontalAlignment="Left" Margin="326,45,0,0" VerticalAlignment="Top" Width="120" IsEnabled="False" SelectionChanged="Select_day"/>
        <Label Content="Год:" HorizontalAlignment="Left" Margin="14,14,0,0" VerticalAlignment="Top"/>
        <Label Content="Месяц:" HorizontalAlignment="Left" Margin="167,14,0,0" VerticalAlignment="Top"/>
        <Label Content="День:" HorizontalAlignment="Left" Margin="326,14,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
